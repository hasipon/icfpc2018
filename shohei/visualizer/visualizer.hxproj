<?xml version="1.0" encoding="utf-8"?>
<project version="2">
  <!-- Output SWF options -->
  <output>
    <movie outputType="CustomBuild" />
    <movie input="" />
    <movie path="visualizer.hxml" />
    <movie fps="0" />
    <movie width="0" />
    <movie height="0" />
    <movie version="0" />
    <movie minorVersion="0" />
    <movie platform="hxml" />
    <movie background="#FFFFFF" />
    <movie preferredSDK="Haxe 3.4.5;3.4.5;" />
  </output>
  <!-- Other classes to be compiled into your SWF -->
  <classpaths>
    <class path="third\react\src\lib" />
    <class path="third\perf,js\1,1,8\src" />
    <class path="third\pixijs\4,7,1\src" />
    <class path="third\tweenxcore\1,0,4" />
    <class path="third\threejs\84,0,0" />
    <class path="src\client" />
    <class path="src\common" />
  </classpaths>
  <!-- Build options -->
  <build>
    <option directives="react_global" />
    <option flashStrict="False" />
    <option noInlineOnDebug="False" />
    <option mainClass="Main" />
    <option enabledebug="False" />
    <option additional="--macro MacroMain.main()" />
  </build>
  <!-- haxelib libraries -->
  <haxelib>
    <!-- example: <library name="..." /> -->
  </haxelib>
  <!-- Class files to compile (other referenced classes will automatically be included) -->
  <compileTargets>
    <compile path="src\client\Main.hx" />
  </compileTargets>
  <!-- Paths to exclude from the Project Explorer tree -->
  <hiddenPaths>
    <hidden path="obj" />
  </hiddenPaths>
  <!-- Executed before build -->
  <preBuildCommand>cmd /c haxe $(OutputFile)</preBuildCommand>
  <!-- Executed after build -->
  <postBuildCommand alwaysRun="False" />
  <!-- Other project options -->
  <options>
    <option showHiddenPaths="False" />
    <option testMovie="Custom" />
    <option testMovieCommand="start.bat" />
  </options>
  <!-- Plugin storage -->
  <storage />
</project>